# EditorConfig (http://EditorConfig.org)

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

max_line_length = 80

indent_style = space
indent_size = 2


[*.md]
trim_trailing_whitespace = false

[COMMIT_EDITMSG]
max_line_length = 0

[*.scala]
max_line_length = 120

[*.php]
indent_size = 4

[*.html]

[*.css]

[*.js]

# Reactjs
[*.jsx]
